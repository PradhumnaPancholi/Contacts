<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="539.0" prefWidth="699.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.CreateContact">
   <children>
      <Label fx:id="Head" layoutX="270.0" layoutY="26.0" text="Profile">
         <font>
            <Font name="Bell MT Bold" size="20.0" />
         </font>
      </Label>
      <VBox layoutX="239.0" layoutY="96.0" prefHeight="265.0" prefWidth="407.0" spacing="10.0">
         <children>
            <Label text="First Name" />
            <TextField fx:id="firstNameTextField" prefHeight="20.0" prefWidth="261.0" VBox.vgrow="NEVER" />
            <Label text="Last Name" />
            <TextField fx:id="lastNameTextField" />
            <Label text="Date Of Birth" />
            <DatePicker fx:id="dateOfBirthDatePicker" prefHeight="22.0" prefWidth="426.0" />
            <Label text="Address" />
            <TextField fx:id="addressTextField" />
            <Label text="Phone" />
            <TextField fx:id="phoneTextField" />
         </children>
         <padding>
            <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
         </padding>
      </VBox>
      <Button fx:id="saveButton" layoutX="504.0" layoutY="447.0" mnemonicParsing="false" onAction="#saveButtonPushed" text="Save" />
      <Button fx:id="cancelButton" layoutX="553.0" layoutY="447.0" mnemonicParsing="false" text="Never Mind" wrapText="true" />
      <ImageView fx:id="imageView" fitHeight="207.0" fitWidth="200.0" layoutX="28.0" layoutY="119.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="chooseImageButton" layoutX="82.0" layoutY="363.0" mnemonicParsing="false" text="Choose Image" />
      <Label fx:id="output" layoutX="252.0" layoutY="490.0" prefHeight="25.0" prefWidth="312.0" />
   </children>
</AnchorPane>
